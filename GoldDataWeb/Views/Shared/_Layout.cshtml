<body style="background-color: @ViewBag.Color;">
	<nav class="navbar-fixed-top" role="navigation" style="padding-top: 10px; position: fixed; z-index: 1030;">
		<div>
			<ul class="nav navbar-nav" style="display: none;">
				<li class="dropdown">
					<a href="#" class="dropdown-toggle" data-toggle="dropdown" style="margin: 0;">
						<div id="countrySelected" style="display: inline-block;">
							<img src="~/Content/Images/flags/16/Venezuela.png" data-country="VE" />
							Venezuela (ES)
						</div>
						<b class="caret"></b>
					</a>
					<ul class="dropdown-menu">
						<li><a class="optionCountry" style="cursor: pointer;"><img src="~/Content/Images/flags/16/Venezuela.png" data-country="VE"> Español (VE)</a></li>
						<li><a class="optionCountry" style="cursor: pointer;"><img src="~/Content/Images/flags/16/Panama.png" data-country="PA"> Panama ()</a></li>

						<li class="divider"></li>
						<li><a style="cursor: pointer;"><img src="~/Content/Images/flags/16/United States of America.png"> E (US) <span class="label label-default">soon</span></a></li>
						<li><a style="cursor: pointer;"><img src="~/Content/Images/flags/16/Germany.png"> Alemania <span class="label label-default">soon</span></a></li>
						<li><a style="cursor: pointer;"><img src="~/Content/Images/flags/16/Portugal.png"> Portugal <span class="label label-default">soon</span></a></li>
						<li><a style="cursor: pointer;"><img src="~/Content/Images/flags/16/Japan.png"> Japon <span class="label label-default">soon</span></a></li>
					</ul>
				</li>
			</ul>
			@{
				if (User.Identity.IsAuthenticated)
				{
					Html.RenderPartial(@"_ToolBar");
				}
			}
		</div><!-- /.container -->
	</nav>

